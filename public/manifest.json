{
  "short_name": "Ai tools",
  "name": "Ai tools - AI Product Description Generator",
  "description": "An AI-powered application to automatically generate structured and professional product descriptions.",
  "id": "/",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": [
    "window-controls-overlay",
    "standalone"
  ],
  "orientation": "any",
  "theme_color": "#1F2937",
  "background_color": "#000000",
  "lang": "en",
  "dir": "ltr",
  "prefer_related_applications": false,
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "share_target": {
    "action": "/?from-share=true",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "image/jpeg": [
          ".jpg",
          ".jpeg"
        ],
        "image/png": [
          ".png"
        ],
        "image/gif": [
          ".gif"
        ],
        "image/webp": [
          ".webp"
        ]
      }
    }
  ],
  "note_taking": {
    "new_note_url": "/?view=notepad&new-note=true"
  },
  "edge_side_panel": {
    "preferred_width": 480
  },
